<%- include a_header %>

<div class="row">

    <div class="col-lg-12">
        <div class="panel panel-default">
            <!--本单元标题-->
            <div class="panel-heading">筛选</div>
            <!--本单元内容-->
            <div class="panel-body">
                <form>
                    关键字：
                    <input type="text">

                    <button class="btn btn-info btn-sm">确定</button>
                </form>
            </div>
        </div>
    </div>

    <div class="col-lg-12">
        <div class="panel panel-default">
            <!--本单元标题-->
            <div class="panel-heading">订单</div>
            <!--本单元内容-->
            <div class="panel-body">
                <table class="table table-hover" style="width: 100%">
                    <thead>
                    <tr>
                        <th>订单类型</th>
                        <th>订单状态</th>
                        <th>订单号</th>
                        <th>收货人</th>
                        <th>订单时间</th>
                        <th>操作</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr>
                        <td>进行中</td><!--进行中/已完成/已关闭-->
                        <td>待发货</td><!--待发货/待付款/已发货/退款中-->
                        <td>xxxxxxxx</td>
                        <td>aciosty</td>
                        <td>yyyy-mm-dd 00:00</td>
                        <td>
                            <a class="btn btn-info" onclick="" href="/a_order_detail">查看订单详情</a>
                        </td>
                    </tr>

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>


<%- include a_footer %>