<%- include a_header %>

<!--START:main-->
<div class="col-sm-12" id="content">
    <div class="row">
        <div class="col-lg-6">
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <!--本单元标题-->
                        <div class="panel-heading">提交操作记录</div>
                        <!--本单元内容-->
                        <div class="panel-body">
                            <div class="form-group">
                                操作内容:<input type="text">
                                <br/>
                                详情：
                                <br/>
                                <textarea style="width: 100%"></textarea>
                                <br/>
                                <button>保存</button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <!--本单元标题-->
                        <div class="panel-heading">操作记录列表</div>
                        <!--本单元内容-->
                        <div class="panel-body">
                            <table class="table table-hover" style="width: 100%">
                                <thead>
                                <tr>
                                    <td>用户名</td>
                                    <td>操作内容</td>
                                </tr>
                                </thead>
                                <tbody>
                                <tr>
                                    <td>illidanswp</td>
                                    <td>进来看看</td>
                                </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>

                </div>
            </div>

        </div>

        <!--确认、功能框-->
        <div class="col-lg-6">
            <div class="panel panel-default">
                <div class="panel-heading">log文档</div>
                <div class="panel-body" style="min-height: 500px">
                    <p>log文档内容</p>
                </div>
            </div>
        </div>
    </div>
</div>
<!--END:main-->

<%- include a_footer %>